    {
      "url": "modules/types/client_interaction/code_executor/",
      "id": 8,
      "title": "Response code",
      "configuration": {
        "sections": {},
        "groups": {
          "group": [
            {
              "display": [
                [
                  "editor",
                  "buttons"
